<!doctype style-sheet PUBLIC "-//James Clark//DTD DSSSL Style Sheet//EN">

<style-sheet>
<style-specification>
<style-specification-body>

(element texto 
  (make simple-page-sequence
   line-number: (literal "6")
   left-header:  (literal "right-header") ))

(element para
  (make paragraph
    font-size: 11pt
    quadding: 'justify
    line-number: (literal "5")  ))

</style-specification-body>
</style-specification>
</style-sheet>
